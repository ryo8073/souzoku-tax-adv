{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "inheritance-tax-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/main.py"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/inheritance-tax-frontend/dist/assets/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/inheritance-tax-frontend/dist/favicon.ico"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/inheritance-tax-frontend/dist/index.html"
    }
  ]
} 